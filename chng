local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("  🥂钢筋脚本🥂   造船寻宝历史活动传送脚本   翻译:坐断你的钢筋")

-- MAIN
local Main = Window:NewTab("不需要传送")
local MainSection = Main:NewSection("自己要是队长要不无法启动")


MainSection:NewButton("打boss(奖励:面包)", "Loads in the fabbie boss (you need to be leader for this to work", function()
    workspace.QuestMakerEvent:FireServer(100)
end)

MainSection:NewButton("姜饼人小游戏(无奖励)", "Loads in the gingerbread Fight (you need to be leader for this to work", function()
    workspace.QuestMakerEvent:FireServer(99)
end)

MainSection:NewButton("雪中小屋小游戏(无奖励)", "Loads in the xmas Fight (you need to be leader for this to work", function()
    workspace.QuestMakerEvent:FireServer(101)
end)

local Main = Window:NewTab("要传送(可能会出现卡顿)")
local MainSection = Main:NewSection("要传送(可能会出现卡顿)")

MainSection:NewButton("去往复活节活动需要打BOSS(奖励:紫色彩弹炮)", "Takes you to the Inner-Cloud", function()    
    local plr = game.Players.LocalPlayer;
local Char = game.Players.LocalPlayer.Character;
local Teleporting = game:GetService("TeleportService");
local CFrameEnd1 = CFrame.new(631, 26, -46);
local Time1 = 5;
local tween1 = game:GetService("TweenService"):Create(Char.HumanoidRootPart, TweenInfo.new(Time1), {CFrame = CFrameEnd1});
local CFrameEnd2 = CFrame.new(484, 68, 270);
local Time2 = 5;
local tween2 =  game:GetService("TweenService"):Create(Char.HumanoidRootPart, TweenInfo.new(Time2), {CFrame = CFrameEnd2});
if game.PlaceId ~= 1930863474 then
Teleporting:Teleport(1930863474, plr)
else
tween1:Play()
tween1.Completed:Wait(X)
end
end)

MainSection:NewButton("传送雪地打boss)(无奖励)", "Takes you to the Winter Place", function()
    local plr = game.Players.LocalPlayer;
local Char = game.Players.LocalPlayer.Character;
local Teleporting = game:GetService("TeleportService");
local CFrameEnd1 = CFrame.new(631, 26, -46);
local Time1 = 5;
local tween1 = game:GetService("TweenService"):Create(Char.HumanoidRootPart, TweenInfo.new(Time1), {CFrame = CFrameEnd1});
local CFrameEnd2 = CFrame.new(484, 68, 270);
local Time2 = 5;
local tween2 =  game:GetService("TweenService"):Create(Char.HumanoidRootPart, TweenInfo.new(Time2), {CFrame = CFrameEnd2});
if game.PlaceId ~= 1930866268 then
Teleporting:Teleport(1930866268, plr)
else
tween1:Play()
tween1.Completed:Wait(X)
end
end)

MainSection:NewButton("传送至下水道(活动已结束无法传送)", "Takes you to the The Secret Place", function()
    local plr = game.Players.LocalPlayer;
local Char = game.Players.LocalPlayer.Character;
local Teleporting = game:GetService("TeleportService");
local CFrameEnd1 = CFrame.new(631, 26, -46);
local Time1 = 5;
local tween1 = game:GetService("TweenService"):Create(Char.HumanoidRootPart, TweenInfo.new(Time1), {CFrame = CFrameEnd1});
local CFrameEnd2 = CFrame.new(484, 68, 270);
local Time2 = 5;
local tween2 =  game:GetService("TweenService"):Create(Char.HumanoidRootPart, TweenInfo.new(Time2), {CFrame = CFrameEnd2});
if game.PlaceId ~= 1930665568 then
Teleporting:Teleport(1930665568, plr)
else
tween1:Play()
tween1.Completed:Wait(X)
end
end)

MainSection:NewButton("什么都没了🥂🥂🥂", "Takes you to the Chamber Of Zeg", function()
    local plr = game.Players.LocalPlayer;
local Char = game.Players.LocalPlayer.Character;
local Teleporting = game:GetService("TeleportService");
local CFrameEnd1 = CFrame.new(631, 26, -46);
local Time1 = 5;
local tween1 = game:GetService("TweenService"):Create(Char.HumanoidRootPart, TweenInfo.new(Time1), {CFrame = CFrameEnd1});
local CFrameEnd2 = CFrame.new(484, 68, 270);
local Time2 = 5;
local tween2 =  game:GetService("TweenService"):Create(Char.HumanoidRootPart, TweenInfo.new(Time2), {CFrame = CFrameEnd2});
if game.PlaceId ~= 5266993598 then
Teleporting:Teleport(5266993598, plr)
else
tween1:Play()
tween1.Completed:Wait(X)
end
end)
